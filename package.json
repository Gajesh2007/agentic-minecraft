{
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "engines": { "node": ">=22" },
  "scripts": {
    "dev:agent": "pnpm --filter agent dev",
    "dev:spectator": "pnpm --filter spectator dev",
    "dev:web": "pnpm --filter web dev",
    "dev": "pnpm --filter agent dev & pnpm --filter spectator dev & pnpm --filter web dev & wait",
    "dev:coordinator": "pnpm --filter coordinator dev",
    "typecheck": "pnpm -r typecheck",
    "build": "pnpm -r build",
    "reset": "rm -rf apps/*/dist packages/*/dist .data && echo 'Reset.'"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
